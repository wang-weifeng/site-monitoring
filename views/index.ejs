<div class="qc-pt-login-content J-commonLoginContent log_window" id="xGpassword" style="width: 100%;display: none" >
    <!--login start-->
    <div class="login-tab" style="position: absolute;left: 0;right: 0;margin: auto;margin-top: 100px">
        <h1 class="login-tab-title J-txtLoginTitle">修改密码</h1>
        <!--邮箱手机登录　start-->
        <div class="login-box J-loginContentBox J-qcloginBox" style="">
            <div class="login-form">
                <div class="tc-msg error" style="display:none">
                    <span class="msg-icon"></span>
                    <div class="tip-info J-loginTip"></div>
                </div>
                <ul class="form-list">
                    <li>       <div class="form-input">
                            <div class="form-unit tip-unit">
                                <label class="input-tips" style="display: none;">输入原密码</label>
                                <input id="old_password" value="" type="text" class="qc-log-input-text lg J-username" placeholder="输入原密码">
                                <ul class="tip-list J-mailSuggest" style="display:none;">
                                </ul>
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="form-input">
                            <div class="form-unit">
                                <label class="input-tips" style="display: none;">输入新密码</label>
                                <input id="new_password" type="password" class="qc-log-input-text lg J-password" placeholder="输入新密码">
                            </div>
                        </div>
                    </li>
                </ul>
            </div>
            <div class="op-btn">
                <input type="button" value="确 定" class="qc-log-btn J-loginBtn" id="determineXigai">
            </div>
            <div class="op-btn">
                <input type="button" value="取 消" class="qc-log-btn J-loginBtn" id="quxiao">            </div>
        </div>
    </div>
</div>  <!--修改密码-->

